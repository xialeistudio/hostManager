{
  "name": "hostManager",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "管理hosts文件",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "hostManager",
    "default_popup": "popup.html"
  },
  "permissions": [
    "system.cpu"
  ],
  "plugins": [
    {
      "path": "./plugins/npapi-file-io-32.dll"
    },
    {
      "path": "./plugins/npapi-file-io-32.so"
    },
    {
      "path": "./plugins/npapi-file-io-64.so"
    }
  ],
  "requirements": {
    "plugins": {
      "npapi": false
    }
  }
}